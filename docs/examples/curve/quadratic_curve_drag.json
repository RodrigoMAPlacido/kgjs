{
  "params": [
    {
    "name": "yintercept",
    "value": 1,
    "min": 0,
    "max": 5,
    "round": 0.01
    }
  ],
  "layout": {
    "type": "OneGraph",
    "def": {
      "graph": {
        "objects": [
          {
            "type": "Curve",
            "def": {
              "univariateFunction": {
                "fn": "params.yintercept + (x)^2",
                "ind": "x"
              },
              "color": "colors.red",
              "label": {
                "text": "`f(x) = ${params.yintercept.toFixed(2)} + x^2`",
                "x": 2
              },
              "drag": [
                {
                  "directions": "y",
                  "param": "yintercept",
                  "expression": "params.yintercept + drag.dy"
                }
              ]
            }
          }
        ]
      }
    }
  }
}