{
  "params": [
    {
      "name": "intercepts",
    "value": 6,
    "min": 2,
    "max": 15,
    "round": 0.01
    }
  ],
  "layout": {
    "type": "OneGraph",
    "def": {
      "graph": {
        "xAxis": {
          "title": "Horizontal Axis",
          "min": 0,
          "max": 15
        },
        "yAxis": {
          "title": "Vertical Axis",
          "min": 0,
          "max": 15
        },
        "objects": [
          {
            "type": "Line",
            "def": {
              "xIntercept": "params.intercepts",
              "yIntercept": "params.intercepts",
              "drag": [
                {
                  "directions": "xy",
                  "param": "intercepts",
                  "expression": "drag.x+drag.y"
                }
              ]
            }
          }
        ]
      }
    }
  }
}