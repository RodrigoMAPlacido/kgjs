{
  "params": [
    {
      "name": "m",
      "value": 1,
      "min": -10,
      "max": 10,
      "round": 0.01
    }
  ],
  "layout": {
    "type": "OneGraph",
    "def": {
      "graph": {
        "xAxis": {
          "title": "Horizontal Axis"
        },
        "yAxis": {
          "title": "Vertical Axis"
        },
        "objects": [
          {
            "type": "Line",
            "def": {
              "color": "colors.green",
              "yIntercept": 4,
              "slope": "params.m",
              "drag": [
                {
                  "directions": "xy",
                  "param": "m",
                  "expression": "(drag.y - 4)/(drag.x)"
                }
              ]
            }
          }
        ]
      }
    }
  }
}
